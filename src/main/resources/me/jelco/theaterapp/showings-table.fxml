<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>
<TableView layoutX="35.0" layoutY="65.0" prefHeight="475.0" prefWidth="928.0" xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1">
    <columns>
        <TableColumn text="Title">
            <cellValueFactory>
                <PropertyValueFactory property="title" />
            </cellValueFactory>
        </TableColumn>
        <TableColumn text="Start">
            <cellValueFactory>
                <PropertyValueFactory property="formattedStartTime" />
            </cellValueFactory>
        </TableColumn>
        <TableColumn text="End">
            <cellValueFactory>
                <PropertyValueFactory property="formattedEndTime" />
            </cellValueFactory>
        </TableColumn>
        <TableColumn text="Room">
            <cellValueFactory>
                <PropertyValueFactory property="roomFormatted" />
            </cellValueFactory>
        </TableColumn>
        <TableColumn text="Age restricted?">
            <cellValueFactory>
                <PropertyValueFactory property="restrictedFormatted" />
            </cellValueFactory>
        </TableColumn>
    </columns>
    <columnResizePolicy>
        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
    </columnResizePolicy>
</TableView>
