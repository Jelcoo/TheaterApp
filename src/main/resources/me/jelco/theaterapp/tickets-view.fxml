<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="610.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label layoutX="35.0" layoutY="35.0" text="Sell tickets">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
        <fx:include fx:id="showsTable" source="showings-table.fxml" />
        <Button fx:id="selectButton" disable="true" layoutX="35.0" layoutY="555.0" mnemonicParsing="false" onAction="#onSelectClick" text="Select seat(s)">
            <font>
                <Font size="16.0" />
            </font>
        </Button>
        <Text fx:id="selectedText" layoutX="162.0" layoutY="575.0" strokeType="OUTSIDE" strokeWidth="0.0" text="No showing selected" />
      <TextField fx:id="searchInput" layoutX="667.0" layoutY="22.0" prefHeight="25.0" prefWidth="293.0" promptText="Search for a show title" />
    </children>
</AnchorPane>
