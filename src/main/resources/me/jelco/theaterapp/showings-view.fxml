<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<?import javafx.scene.control.cell.PropertyValueFactory?>
<AnchorPane prefHeight="610.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label layoutX="35.0" layoutY="35.0" text="Manage shows">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <TableView fx:id="showsTable" layoutX="35.0" layoutY="65.0" prefHeight="510.0" prefWidth="928.0">
        <columns>
            <TableColumn text="Title">
                <cellValueFactory>
                    <PropertyValueFactory property="title" />
                </cellValueFactory>
            </TableColumn>
            <TableColumn text="Start">
                <cellValueFactory>
                    <PropertyValueFactory property="start" />
                </cellValueFactory>
            </TableColumn>
            <TableColumn text="End">
                <cellValueFactory>
                    <PropertyValueFactory property="end" />
                </cellValueFactory>
            </TableColumn>
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</AnchorPane>
